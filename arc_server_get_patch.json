{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "arcServerId": {
      "type": "string"
    }
  },
  "functions": [],
  "variables": {

  },
  "resources": [
    {
      "name": "linkedTemplate",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
        "relativePath": "linked_template.json"
        },
        "parameters": {
          "arcServerId": {
            "value": "[parameters('arcServerId')]"
          }
        }
      }
    },
    {
      "apiVersion": "2022-03-10",
      "name": "[concat(parameters('arcServerId'), '/WindowsAgent.SqlServer')]",
      "type": "Microsoft.HybridCompute/machines/extensions",
      "location": "eastus2euap",
      "properties": {
        "publisher": "[reference('linkedTemplate').outputs.agentExtensionBody.properties.publisher]",
        "type": "[reference('linkedTemplate').outputs.agentExtensionBody.properties.type]",
        "typeHandlerVersion": "[reference('linkedTemplate').outputs.agentExtensionBody.properties.typeHandlerVersion]",
        "autoUpgradeMinorVersion": "[reference('linkedTemplate').outputs.agentExtensionBody.properties.autoUpgradeMinorVersion]",
        "enableAutomaticUpgrade": "[reference('linkedTemplate').outputs.agentExtensionBody.properties.enableAutomaticUpgrade]",
        "settings": "[reference('linkedTemplate').outputs.agentExtensionBody.properties.settings]"
      }
    }
  ],
  "outputs": {

  }
}